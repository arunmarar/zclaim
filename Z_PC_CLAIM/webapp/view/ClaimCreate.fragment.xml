	<f:SimpleForm id="formClaimCreate"
			editable="true"
			layout="ResponsiveLayout"
			title="Claim" >
			<f:content>
			
				
					<Label text="Item code" required="true" />
							<Input id="itemCodeClaimCreate" showSuggestion="true" showValueHelp="true" valueHelpRequest="onPressShowDialog"/>
				
				<Label text="Item Description" />
				<Input id="itemDescClaimCreate" enabled="false" />
				
				<Label text="No of Items" required="true" />
				<Input value="" id="numItemClaimCreate"  type="Text" maxLength="40"   placeholder="Max 40 digits"  />
				<Label text="Bill Number" required="true" />
				<Input value="" id="billNumClaimCreate"  type="Text" maxLength="20"  placeholder="Max 20 digits"  />
				<Label text="GST No of Gail Unit" />
				<Input value="" id="gstNumClaimCreate"    type="Text" maxLength="40" placeholder="Max 40 digits"  />
				<Label text="GST No of Vendor" />
				<Input value="" id="gstNumVClaimCreate"  type="Text" maxLength="40"   placeholder="Max 40 digits"  />
				<Label text="GST Tax Rate" />
				<Input value="" id="gstTaxRateClaimCreate"  type="Number" liveChange="onChangeDecimal"  placeholder="Max 15 digits"  />
				<Label text="Base Value" />
				<Input value="" id="baseValueClaimCreate"    type="Number"  liveChange="onChangeDecimal" placeholder="Max 15 digits"  />
				<Label text="CGST Amount" />
				<Input value="" id="cgstClaimCreate"    type="Number" liveChange="onChangeDecimal"  placeholder="Max 15 digits"  />
				<Label text="SGST Amount" />
				<Input value="" id="sgstClaimCreate"    type="Number"  liveChange="onChangeDecimal" placeholder="Max 15 digits" />
				
				<Label text="IGST Amount" />
				<Input value="" id="igstClaimCreate"   type="Number" liveChange="onChangeDecimal"  placeholder="Max 15 digits"  />
					<Label text="UGST Amount" />
				<Input value="" id="ugstClaimCreate"    type="Number" liveChange="onChangeDecimal"  placeholder="Max 15 digits"  />
				<Label text="HSN/SAC Code" />
				<Input value="" id="hsnCodeClaimCreate"     type="Text" maxLength="20"  placeholder="Max 20 digits"  />
				<Label text="Vendor Name" />
				<Input value="" id="venNameClaimCreate"     type="Text" maxLength="40"  placeholder="Max 40 digits" />
				<Label text="Bill Date" required="true" />
				<DatePicker id="billDateClaimCreate"  />
				<Label text="Bill/Settelment Amount" required="true" />
				<Input value="" id="billSAmtClaimCreate"  type="Number"  liveChange="onChangeDecimal" placeholder="Max 15 digits"  />
			
				<Label text="Requested Amount" required="true" />
				<Input value="" id="reqAmtClaimCreate"  type="Number" liveChange="onChangeDecimal"  placeholder="Max 15 digits"   />
				
				
				
			</f:content>
		</f:SimpleForm>
