<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<!--<VBox class="sapUiSmallMargin">-->
		<f:SimpleForm id="formClaimAgainstAdvanceDisplay"
			editable="false"
			layout="ResponsiveLayout"
			title="Claim Against Advance" >
			<f:content>
				<Label text="Advance" />
			
				<Text text="{detailMasterModel>ASREF}"  />
					<Label text="Item code" />
				<Text text="{detailMasterModel>C10T5}"  />
				<!--<Label text="Item Description" />-->
				<!--<Text text="{detailMasterModel>C40T3}"  />-->
				<Label text="No of Items" />
				<Text text="{detailMasterModel>C40T2}"  />
				<Label text="Bill Number" />
				<Text text="{detailMasterModel>C20T1}"  />
				<Label text="GST No of Gail Unit" />
				<Text text="{detailMasterModel>C40T5}"  />
				<Label text="GST No of Vendor" />
				<Text text="{detailMasterModel>C40T1}"  />
				<Label text="GST Tax Rate" />
				<Text text="{detailMasterModel>AMT07}"  />
				<Label text="Base Value" />
				<Text text="{detailMasterModel>AMT06}"  />
			<Label text="CGST Amount" />
				<Text text="{detailMasterModel>AMT02}"  />      
				<Label text="SGST Amount" />
				<Text text="{detailMasterModel>AMT04}"  />
				
				<Label text="IGST Amount" />
			<Text text="{detailMasterModel>AMT05}"  />
					<Label text="UGST Amount" />
				<Text text="{detailMasterModel>AMT03}"  />
				<Label text="HSN/SAC Code" />
			<Text text="{detailMasterModel>C20T2}" />
				<Label text="Vendor Name" />
				<Text text="{detailMasterModel>C40T3_V}" />
				<Label text="Bill Date" />
				<Text text="{
						path:'detailMasterModel>CDT01',
						formatter:'.formatter.formatReqDate'
						}" />
				<Label text="Bill/Settelment Amount" />
				<Text text="{detailMasterModel>AMT01}" />
			
				<Label text="Requested Amount" />
			<Text text="{detailMasterModel>RQAMT}" />
				
			</f:content>
		</f:SimpleForm>
	<!--</VBox>-->
</core:FragmentDefinition>